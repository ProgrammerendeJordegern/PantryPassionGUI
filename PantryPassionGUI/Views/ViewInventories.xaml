<Window x:Class="PantryPassionGUI.Views.ViewInventories"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PantryPassionGUI.Views"
        xmlns:viewModels="clr-namespace:PantryPassionGUI.ViewModels"
        mc:Ignorable="d"
        Title="Vis" Height="450" Width="800">

    <Window.DataContext>
        <viewModels:ViewInventoriesViewModel></viewModels:ViewInventoriesViewModel>
    </Window.DataContext>

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="22*"/>
            <RowDefinition Height="195*"/>
        </Grid.RowDefinitions>
        <DataGrid Grid.Row="1" Margin="10,0,10,10"/>
        <ComboBox ItemsSource="{Binding Path=CMBBX}" SelectedIndex="0" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="222" Height="22"/>

    </Grid>
</Window>
